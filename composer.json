{
    "name": "heimseiten/contao-custom-backend-settings-bundle",
    "description": "Contao-Custom-Backend-Settings",
    "keywords": [
        "Kopie Label",
        "CSS Feld",
        "News kopieren",
        "Events kopieren"
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "heimseiten.de - Webdesign aus KÃ¶ln",
            "email": "info@heimseiten.de",
            "homepage": "https://www.heimseiten.de/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/heimseiten/contao-custom-backend-settings-bundle/issues",
        "source": "https://github.com/heimseiten/contao-custom-backend-settings-bundle"
    },
    "require": {
        "contao/core-bundle": "^4.9"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^4.0",
        "contao/manager-plugin": "^2.3.1",
        "contao/test-case": "^4.6",
        "phpunit/phpunit": "^9.3",
        "symfony/phpunit-bridge": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Heimseiten\\ContaoCustomBackendSettingsBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Heimseiten\\ContaoCustomBackendSettingsBundle\\ContaoManager\\Plugin"
    }
}
